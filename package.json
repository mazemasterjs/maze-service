    "dependencies": {
        "@mazemasterjs/logger": "^1.0.3",
        "@mazemasterjs/shared-library": "^1.2.4",
        "compression": "^1.7.3",
        "ejs": "^2.6.1",
        "express": "^4.16.4",
        "mongodb": "^3.1.13",
        "path-exists": "^3.0.0"
    },
    "devDependencies": {
        "@types/compression": "0.0.36",
